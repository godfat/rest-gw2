<%= erb :menu_guild, arg %>

<h2>Members</h2>
<table>
  <tr>
    <th>Name</th>
    <th>Rank</th>
    <th>Joined</th>
  </tr>
  <% arg[:members].each do |member| %>
  <tr>
    <td><%= h member['name'] %></td>
    <td><%= h member['rank'] %></td>
    <td>
      <% if member['joined'] %>
        <%= abbr_time_ago(member['joined']) %>
      <% else %>
        (unknown)
      <% end %>
    </td>
  </tr>
  <% end %>
</table>
